<h5 class="mat-h5">@this.DocInfo.SectionText</h5>
<DemoContainer>
    <Content>
        @ChildContent
    </Content>
    <SourceContent>
        <BlazorFiddle Template="MatBlazor" Code=@(this.DocInfo.Code)></BlazorFiddle>
    </SourceContent>
</DemoContainer>

@code{

    [Parameter]
    public RenderFragment ChildContent { get; set; }

    [Parameter]
    public DocInfo DocInfo { get; set; }

}
