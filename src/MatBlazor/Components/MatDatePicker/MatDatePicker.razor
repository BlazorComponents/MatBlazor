@namespace MatBlazor
@inherits BaseMatDatePicker


<MatTextField
    RefBack="@RefBack"
    Class="@Class"
    Style="@Style"
    Icon="@MatIconNames.Calendar_today"
    IconTrailing="true"
    Label="@Label"
    Dense="@Dense"
    HelperText="@HelperText"
    Outlined="@Outlined"
    FullWidth="@FullWidth"
    PlaceHolder="@PlaceHolder"
    Required="@Required"
    Disabled="@Disabled"
    ReadOnly="@ReadOnly"
    Attributes="@Attributes"
    Id="@Id"
    @ref ="TextFieldRef">
</MatTextField>